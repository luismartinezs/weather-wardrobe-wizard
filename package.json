{
  "name": "weather-wardrobe-wizard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf node_modules pnpm-lock.yaml",
    "clean-cache": "rm -rf .next",
    "plop": "plop  --plopfile ./generators/plopfile.mjs",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --passWithNoTests",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "emu:clean": "rm -rf emu-data",
    "emu:dev": "cross-env NEXT_PUBLIC_FIREBASE_EMULATOR=true next dev",
    "emu:start": "firebase emulators:start --import=./emu-data --export-on-exit",
    "firebase:deploy": "firebase deploy",
    "build:functions": "cd functions && pnpm build",
    "fb:deploy:rules": "firebase deploy --only firestore:rules",
    "fb:deploy:functions": "firebase deploy --only functions"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.7.1",
    "@chakra-ui/skip-nav": "^2.0.15",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.1",
    "@next/font": "13.4.7",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "firebase": "^9.23.0",
    "firebase-admin": "^11.9.0",
    "firebase-functions": "^4.4.1",
    "framer-motion": "^10.12.17",
    "immer": "^10.0.2",
    "lodash.debounce": "^4.0.8",
    "next": "13.4.7",
    "next-pwa": "^5.6.0",
    "p-retry": "^5.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "react-icons": "^4.10.1",
    "react-query": "^3.39.3",
    "recharts": "^2.7.2",
    "styled-jsx": "^5.1.2",
    "typescript": "5.1.3",
    "uuid": "^9.0.0",
    "yup": "^1.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/uuid": "^9.0.2",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "plop": "^3.1.2",
    "prettier": "2.8.8"
  }
}
